class myPhoto {
	public var image;
	public var caption_text:String;
	public var credit_text:String;	

	public function setImage(image) {
		this.image = image;		
	}
	public function setCaption(caption_text){
		if(this.caption_text = caption_text) {
			return true;
		}
	}
	public function setCredit(credit_text){
		if(this.credit_text = credit_text) {
			return true;
		}
	}
}